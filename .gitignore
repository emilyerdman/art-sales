# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
tmp
log
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
*.DS_Store

# ignore database setup
config/database.yml

# ignore development environment details
config/environments/development.rb

# ignore database backups
backups/

# ignore seed files
lib/seeds/*

public/assets/image
public/assets/killipsian\ fine\ art
public/assets/poster\ images\ for\ web


# Ignore encrypted secrets key file.
config/secrets.yml.key

# ignore localhost ssl setup
config/ssl

# ignore numbers files
*.numbers
*.xls



/config/master.key
